package com.revature.Project2.beans;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table
public class MacroNutrients {

	// Primary Key for Users Table in Oracle DB
	@Id
	@Column(name = "N_ID")
	private int nId;

	@Column
	private int minAge;
	
	@Column
	private int maxAge;

	@Column(name = "SEX")
	private int sex;

	@Column
	private int protein;

	@Column
	private double proteinMinPercent;
	
	@Column
	private double proteinMaxPercent;

	@Column
	private int carbohydrate;

	@Column
	private double carbohydrateMinPercent;
	
	@Column
	private double carbohydrateMaxPercent;

	@Column
	private double dietaryFiber;

	@Column
	private double addedSugars;

	@Column
	private int totalFat;

	@Column
	private int saturatedFat;

	public MacroNutrients() {
	}

	
}
