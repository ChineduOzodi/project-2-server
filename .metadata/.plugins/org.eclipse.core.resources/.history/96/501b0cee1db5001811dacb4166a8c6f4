package com.revature.Project2.beans;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table
public class MacroNutrients {

	// Primary Key for Users Table in Oracle DB
	@Id
	@Column(name = "U_ID")
	private int uId;

	@Column(name = "AGE")
	private int age;

	@Column(name = "SEX")
	private int sex;

	@Column
	private int protein;

	@Column
	private double proteinPercent;

	@Column
	private int carbohydrate;

	@Column
	private double carbohydratePercent;

	@Column
	private double dietaryFiber;

	@Column
	private double addedSugars;

	@Column
	private int totalFat;

	@Column
	private int saturatedFat;

	@Column
	private int LinoleicAcid;

	@Column
	private double LinolenicAcid;

	public MacroNutrients() {
	}

	public MacroNutrients(int uId, int age, int sex, int protein, double proteinPercent, int carbohydrate,
			double carbohydratePercent, double dietaryFiber, double addedSugars, int totalFat, int saturatedFat,
			int linoleicAcid, double linolenicAcid) {
		super();
		this.uId = uId;
		this.age = age;
		this.sex = sex;
		this.protein = protein;
		this.proteinPercent = proteinPercent;
		this.carbohydrate = carbohydrate;
		this.carbohydratePercent = carbohydratePercent;
		this.dietaryFiber = dietaryFiber;
		this.addedSugars = addedSugars;
		this.totalFat = totalFat;
		this.saturatedFat = saturatedFat;
		LinoleicAcid = linoleicAcid;
		LinolenicAcid = linolenicAcid;
	}

	public int getuId() {
		return uId;
	}

	public void setuId(int uId) {
		this.uId = uId;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getSex() {
		return sex;
	}

	public void setSex(int sex) {
		this.sex = sex;
	}

	public int getProtein() {
		return protein;
	}

	public void setProtein(int protein) {
		this.protein = protein;
	}

	public double getProteinPercent() {
		return proteinPercent;
	}

	public void setProteinPercent(double proteinPercent) {
		this.proteinPercent = proteinPercent;
	}

	public int getCarbohydrate() {
		return carbohydrate;
	}

	public void setCarbohydrate(int carbohydrate) {
		this.carbohydrate = carbohydrate;
	}

	public double getCarbohydratePercent() {
		return carbohydratePercent;
	}

	public void setCarbohydratePercent(double carbohydratePercent) {
		this.carbohydratePercent = carbohydratePercent;
	}

	public double getDietaryFiber() {
		return dietaryFiber;
	}

	public void setDietaryFiber(double dietaryFiber) {
		this.dietaryFiber = dietaryFiber;
	}

	public double getAddedSugars() {
		return addedSugars;
	}

	public void setAddedSugars(double addedSugars) {
		this.addedSugars = addedSugars;
	}

	public int getTotalFat() {
		return totalFat;
	}

	public void setTotalFat(int totalFat) {
		this.totalFat = totalFat;
	}

	public int getSaturatedFat() {
		return saturatedFat;
	}

	public void setSaturatedFat(int saturatedFat) {
		this.saturatedFat = saturatedFat;
	}

	public int getLinoleicAcid() {
		return LinoleicAcid;
	}

	public void setLinoleicAcid(int linoleicAcid) {
		LinoleicAcid = linoleicAcid;
	}

	public double getLinolenicAcid() {
		return LinolenicAcid;
	}

	public void setLinolenicAcid(double linolenicAcid) {
		LinolenicAcid = linolenicAcid;
	}

	@Override
	public String toString() {
		return "MacroNutrients [uId=" + uId + ", age=" + age + ", sex=" + sex + ", protein=" + protein
				+ ", proteinPercent=" + proteinPercent + ", carbohydrate=" + carbohydrate + ", carbohydratePercent="
				+ carbohydratePercent + ", dietaryFiber=" + dietaryFiber + ", addedSugars=" + addedSugars
				+ ", totalFat=" + totalFat + ", saturatedFat=" + saturatedFat + ", LinoleicAcid=" + LinoleicAcid
				+ ", LinolenicAcid=" + LinolenicAcid + "]";
	}
}
